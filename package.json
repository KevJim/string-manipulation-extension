{
    "name": "strmanip",
    "displayName": "String Manipulation",
    "description": "Port of JetBrains String Manipulation plugin for VS Code. Provides case switching (camelCase, snake_case, kebab-case, etc.) and text alignment (vertical alignment, columns, justification) utilities.",
    "version": "0.0.1",
    "publisher": "kevinjimenez",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/KevJim/string-manipulation-extension.git"
    },
    "author": "kevinjimenez",
    "engines": {
        "vscode": "^1.74.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:strmanip.*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "strmanip.toCamel",
                "title": "To camelCase",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toSnake",
                "title": "To snake_case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toKebab",
                "title": "To kebab-case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toPascal",
                "title": "To PascalCase",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toConstant",
                "title": "To SCREAMING_SNAKE_CASE",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toDot",
                "title": "To dot.case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toTitle",
                "title": "To Title Case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toSentence",
                "title": "To Sentence case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toUpper",
                "title": "To UPPER CASE",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toLower",
                "title": "To lower case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.invertCase",
                "title": "Invert Case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.toggleCase",
                "title": "Toggle Case",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.alignVertical",
                "title": "Align Carets Vertically",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.alignColumns",
                "title": "Align to Columns (Separator)",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.justifyLeft",
                "title": "Justify Left",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.justifyCenter",
                "title": "Justify Center",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.justifyRight",
                "title": "Justify Right",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.showQuickPick",
                "title": "Show String Manipulation Menu",
                "category": "String Manipulation"
            },
            {
                "command": "strmanip.cycleCase",
                "title": "Cycle Case",
                "category": "String Manipulation"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "submenu": "strmanip.menu",
                    "group": "modification",
                    "when": "editorTextFocus"
                }
            ],
            "strmanip.menu": [
                {
                    "command": "strmanip.toCamel",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toSnake",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toKebab",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toPascal",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toConstant",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toDot",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toTitle",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toSentence",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toUpper",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toLower",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.invertCase",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.toggleCase",
                    "group": "1_casing"
                },
                {
                    "command": "strmanip.alignVertical",
                    "group": "2_alignment"
                },
                {
                    "command": "strmanip.alignColumns",
                    "group": "2_alignment"
                },
                {
                    "command": "strmanip.justifyLeft",
                    "group": "2_alignment"
                },
                {
                    "command": "strmanip.justifyCenter",
                    "group": "2_alignment"
                },
                {
                    "command": "strmanip.justifyRight",
                    "group": "2_alignment"
                }
            ]
        },
        "submenus": [
            {
                "id": "strmanip.menu",
                "label": "String Manipulation"
            }
        ],
        "keybindings": [
            {
                "command": "strmanip.cycleCase",
                "key": "alt+shift+u",
                "mac": "alt+shift+u",
                "when": "editorTextFocus"
            },
            {
                "command": "strmanip.showQuickPick",
                "key": "ctrl+shift+u",
                "mac": "cmd+shift+u",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "node esbuild.mjs",
        "watch": "node esbuild.mjs --watch",
        "pretest": "npm run compile && npm run lint",
        "test": "npm run compile && npm run compile:tests && node ./out/test/runTest.js",
        "compile:tests": "tsc -p tsconfig.test.json",
        "test:unit": "vitest run",
        "test:unit:watch": "vitest",
        "lint": "eslint src --ext ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "typecheck": "tsc --noEmit --incremental"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "4.3.0",
        "@types/glob": "8.1.0",
        "@types/mocha": "10.0.10",
        "@types/node": "18.x",
        "@types/vscode": "1.74.0",
        "@typescript-eslint/eslint-plugin": "6.0.0",
        "@typescript-eslint/parser": "6.0.0",
        "@vscode/test-electron": "2.5.2",
        "esbuild": "0.27.0",
        "eslint": "8.42.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-import-resolver-typescript": "3.6.3",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-no-relative-import-paths": "1.5.5",
        "eslint-plugin-prettier": "5.2.1",
        "glob": "13.0.0",
        "mocha": "11.7.5",
        "prettier": "3.0.0",
        "typescript": "5.1.6",
        "vitest": "1.4.0"
    },
    "dependencies": {
        "change-case": "4.1.2"
    }
}
